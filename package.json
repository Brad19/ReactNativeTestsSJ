  "dependencies": {
    "date-fns": "^2.17.0",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-navigation": "^7.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
